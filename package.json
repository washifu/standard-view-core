{
  "name": "@standard/view",
  "version": "1.0.3",
  "description": "3D Graphics Library for React Apps",
  "repository": "Standard-Cognition/standard-view-core",
  "license": "MIT",
  "contributors": [
    "Standard Cognition (https://standard.ai/) <jobs@standard.ai>",
    "Wasif S. Islam",
    "Maytee Chinavanichkit",
    "Francisco Presencia (https://francisco.io/)"
  ],
  "main": "./index.js",
  "scripts": {
    "build": "yarn build-webpack && yarn build-babel && yarn build-tsc",
    "build-webpack": "webpack --mode production",
    "build-webpack-dev": "webpack --mode development --output-path lib/",
    "build-babel": "babel --extensions '.ts,.tsx,.js,.jsx' src/ -d lib/ --copy-files",
    "build-storybook": "build-storybook -s ./stories/assets/",
    "build-tsc": "tsc",
    "build-flow": "flow-copy-source src lib",
    "flow": "flow",
    "lint": "eslint --cache --format codeframe --ext ts,tsx,mjs,jsx,js src stories",
    "lint-fix": "eslint --cache --format codeframe --ext mjs,jsx,js src stories --fix",
    "test": "jest",
    "prepublish": "yarn build",
    "storybook": "start-storybook -s ./stories/assets/ -p 5000"
  },
  "keywords": [
    "standard-view",
    "standard-view-core",
    "standard",
    "view",
    "core",
    "react-three-fiber",
    "three",
    "three.js",
    "visualization",
    "tool",
    "package",
    "3D",
    "graphics"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@neutrinojs/airbnb": "^9.0.0-rc.3",
    "@neutrinojs/jest": "^9.0.0-rc.3",
    "@neutrinojs/react-components": "^9.0.0-rc.3",
    "@storybook/addon-actions": "5.0.11",
    "@storybook/addon-knobs": "5.0.11",
    "@storybook/addon-links": "5.0.11",
    "@storybook/addons": "5.0.11",
    "@storybook/react": "5.0.11",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.3",
    "neutrino": "^9.0.0-rc.3",
    "prettier": "^1.17.1",
    "prop-types": "^15.7.2",
    "prop-types-exact": "^1.2.0",
    "react": "^16.9",
    "react-dom": "^16.9",
    "react-three-fiber": "^4.1.1",
    "three": "^0.125.0",
    "typescript": "^3.5.3",
    "webpack": "^4",
    "webpack-cli": "^3",
    "webpack-dev-server": "^3"
  },
  "peerDependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "prop-types-exact": "^1.2.0",
    "react": "^16.9",
    "react-three-fiber": "^4.1.1",
    "three": "^0.125.0"
  },
  "dependencies": {
    "multivariate-normal": "^0.1.2",
    "stats.js": "^0.17.0"
  }
